<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form"
	xmlns:smartForm="sap.ui.comp.smartform" xmlns:smartField="sap.ui.comp.smartfield" xmlns="sap.m">
	<Dialog id="idDialogDeleteWC" class="sapUiSizeCompact" title="{i18n>TitleDeleteWCAssignmentDialog}" state="Warning" contentWidth="10rem" type="Message">
		<content>
			<Text id="idTextWC" text="{parts:[{path:'i18n>DeleteWCAssignmentMessage1'}, {path:'WorkCenter'}], formatter:'jQuery.sap.formatMessage'}"/>
		</content>
		<beginButton>
			<Button id="idBtnOk" text="{i18n>DeleteWCButton}" press="onDeleteWCOKPress"/>
		</beginButton>
		<endButton>
			<Button id="idBtnCancel" text="{i18n>CancelButton}" press="onDeleteWCClosePress"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>