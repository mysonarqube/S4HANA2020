<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
	<Dialog id="idChangePriorityDailog" title="{i18n>ChangePriority}">
		<VBox id="idVBoxChangePriority" alignItems="Center">
			<StepInput id="idPriority" min="0" max="32767" validationMode="LiveChange" change="onPriorityChange" textAlign="Center"/>
		</VBox>
		<buttons>
			<Button id="idPrioritySave" text="{i18n>saveButtonText}" press="onChangePriorityDialogSave" enabled="true" type="Emphasized"/>
			<Button id="idPriorityCancel" text="{i18n>cancelButtonText}" press="onChangePriorityDialogCancel"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>