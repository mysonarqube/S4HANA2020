<core:FragmentDefinition
		xmlns="sap.m"
		xmlns:smartfilterbar="sap.ui.comp.smartfilterbar"
		xmlns:core="sap.ui.core">
	
	<smartfilterbar:ControlConfiguration id="CustomFilterSmartFilterBarConfOpLtstSchedldExecStrtDte" key="OpLtstSchedldExecStrtDte" index="9" 
		conditionType="{module:'sap.ui.comp.config.condition.DateRangeType', 
		operations: { filter: {path: 'key', contains: 'FROM,TO,DATERANGE,TODAY,LASTDAYS,NEXTDAYS,LASTWEEK,LASTWEEKS,NEXTWEEK,NEXTWEEKS,MONTH,LASTMONTH,LASTMONTHS,LASTYEAR,LASTYEARS,THISYEAR,NEXTYEAR,NEXTYEARS'}}}"
		label="{path:'/#C_ProcgExecOpActyInstceTP/OpLtstSchedldExecStrtDte/@sap:label'}"
		visibleInAdvancedArea="true" groupId="C_ProcgExecOpActyInstceTP">
	</smartfilterbar:ControlConfiguration>
	
	<smartfilterbar:ControlConfiguration id="CustomFilterSmartFilterBarConfOpActyNtwkSegmentType" key="OpActyNtwkSegmentType" index="10"
		label="{path:'/#C_ProcgExecOpActyInstceTP/OpActyNtwkSegmentType/@sap:label'}"
		visibleInAdvancedArea="false" groupId="C_ProcgExecOpActyInstceTP">
		<smartfilterbar:customControl>
			<MultiComboBox id="OpActyNtwkSegmentType-combobox" selectionChange="onSelectionChange">
				<core:Item id="OpActyNtwkSegmentTypeItem0" key="0" 
						text="{i18n|sap.suite.ui.generic.template.ListReport|C_ProcgExecOpActyInstceTP>xtit.AllSegmentTypes}"/>
				<core:Item id="OpActyNtwkSegmentTypeItem1" key="1" 
						text="{i18n|sap.suite.ui.generic.template.ListReport|C_ProcgExecOpActyInstceTP>xtit.Setup}"/>
				<core:Item id="OpActyNtwkSegmentTypeItem2" key="2" 
						text="{i18n|sap.suite.ui.generic.template.ListReport|C_ProcgExecOpActyInstceTP>xtit.Produce}"/>
				<core:Item id="OpActyNtwkSegmentTypeItem3" key="3" 
						text="{i18n|sap.suite.ui.generic.template.ListReport|C_ProcgExecOpActyInstceTP>xtit.Teardown}"/>
			</MultiComboBox>
		</smartfilterbar:customControl>
	</smartfilterbar:ControlConfiguration>
		
	
</core:FragmentDefinition>